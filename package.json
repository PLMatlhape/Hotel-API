{
  "name": "hotel-booking-backend-enterprise",
  "version": "2.0.0",
  "description": "Enterprise-grade Hotel Booking API with TypeScript, PostgreSQL, and Redis",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
  "dev": "nodemon --watch src --ext ts,js --exec \"node --loader ts-node/esm src/app.ts\"",
    "build": "tsc",
    "start": "node dist/app.js",
  "start:dev": "node --loader ts-node/esm src/app.ts",
    "start:prod": "NODE_ENV=production node dist/app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "docker:build": "docker build -t hotel-booking-api .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "hotel",
    "booking",
    "typescript",
    "postgresql",
    "redis",
    "stripe",
    "paypal",
    "enterprise"
  ],
  "author": "Your Company",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@paypal/checkout-server-sdk": "^1.0.3",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-slow-down": "^2.0.1",
    "express-useragent": "^1.0.15",
    "express-validator": "^7.0.1",
    "flutterwave-node-v3": "^1.0.10",
    "geoip-lite": "^1.4.7",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "sharp": "^0.33.1",
    "stripe": "^14.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
